{
  "name": "@unikum/base-nodejs",
  "version": "0.1.25",
  "description": "base necessities for a nodejs typescript project",
  "publishConfig": {
    "registry": "https://nexus.unikum.us/repository/unikum-npm"
  },
  "engines": {
    "node": ">=16"
  },
  "main": "./dist/src/index.js",
  "type": "commonjs",
  "scripts": {
    "build": "tsc -p tsconfig.build.json",
    "lint": "eslint . --fix",
    "prepare:release": "concurrently 'npm run test:coverage' 'npm run lint' 'npm run build'",
    "setup:project": "ts-node ./internals/setup-project.ts",
    "release": "standard-version",
    "test": "jest",
    "test:coverage": "jest --coverage",
    "test:watch-coverage": "jest --coverage --watch",
    "test:watch": "jest --watchAll",
    "prepare": "husky install && ts-patch install -s"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@swc/cli": "^0.1.57",
    "@swc/core": "^1.2.171",
